module package

import "platform.pkl"

class Target {
  name: String
  cmd: String?
  deps: Listing<String>(isDistinct)
  inputs:  Listing<String>(isDistinct)
  outputs: Listing<String>(isDistinct)
  bin_output: String?

  // Target Filtering
  tags: Listing<String>(isDistinct)
  platform: platform?
}


targets: Listing<Target>(isDistinctBy((it) -> it.name))
