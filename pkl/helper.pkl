import "target.pkl"

function java_app(
  inputName: String
) = new target.Target {
  name = inputName
  cmd = "build"
}


function java_app_multiple_targets(
  inputName: String
) = new Listing<target.Target> {
  new {
    name = inputName
    cmd = "build"
  }
  new {
    name = inputName + "_test"
    cmd = "test"
  }
}
